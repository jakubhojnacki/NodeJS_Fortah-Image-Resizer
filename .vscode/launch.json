{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/main.mjs",
            "args": [                
                "/home/Temp/From",
                "/home/Temp/To",
                "16,24,32,48,64,128,256",
                "-sfm", "*.png",
                "-dt", "{3}",
                "-ft", "{0}",
                "-__de"
            ],
            "console": "integratedTerminal"
        }
    ]
}